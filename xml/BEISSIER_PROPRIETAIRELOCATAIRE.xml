	<front>
		<docTitle>
			<titlePart type="main">PROPRIÉTAIRE ET LOCATAIRE</titlePart>
			<titlePart type="sub">- GUIGNOL LOCATAIRE -</titlePart>
			<titlePart type="sub">THÉÂTRE DE GUIGNOL.</titlePart>
			</docTitle> 
		<docDate value="1894">1894.</docDate>
		<docAuthor id="BESSIER, Fernand">Fernand BESSIER</docAuthor>
		<docImprint>
			<editor>PARIS, LIBRAIRIE THÉÂTRALE, 14 rue Grammont, 14.</editor>
		</docImprint>
	<castList><head>PERSONNAGES.</head>
		<castItem><role id="GUIGNOL" sex="1" type="H" statut="bourgeois" age="A" stat_amour="néant">GUIGNOL</role>.</castItem>
		<castItem><role id="LE CONCIERGE" sex="1" type="H" statut="concierge" age="A" stat_amour="néant">LE CONCIERGE</role>.</castItem>
		<castItem><role id="MAÎTRE FURET" sex="1" type="H" statut="huissier" age="V" stat_amour="néant">MAÎTRE FURET</role>.</castItem>
		<castItem><role id="LE PROPRIÉTAIRE" sex="1" type="H" statut="apothicaire" age="A" stat_amour="néant">LE PROPRIÉTAIRE</role>.</castItem>
		<castItem><role id="LE GENDARME" sex="1" type="H" statut="bourgeois" age="A" stat_amour="néant">LE GENDARME</role>.</castItem>
